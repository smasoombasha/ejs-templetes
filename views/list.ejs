<%- include ("header"); -%>

<div class="box" id="heading">
  <h1><%= ListOfTitle %></h1>
</div>


<div class="box">
  <%  for(let i =0;i<newListItems.length;i++){ %>
  <div class="item">
    <input type="checkbox">
    <p><%= newListItems[i] %></p>
  </div>
  <%  } %>

  <form class="item" action="/" method="post">
    <input type="text" name="newItem" placeholder="NewItem" autofocus autocomplete="off">
    <button type="submit" name="list" value=<%= ListOfTitle %>>+</button>
  </form>
</div>


<footer>
  Copyright 2019 masoom's
</footer>

<%- include ("footer"); -%>
